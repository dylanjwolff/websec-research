{
  "manifest_version": 2,

  "name": "XSS Sec",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],

    "content_scripts": [
        {
            "js": ["inject.js"],
            "matches": ["<all_urls>"],
            "run_at": "document_start"
        }
    ]
}
